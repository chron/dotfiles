[color]
  ui = true
  status = auto
[user]
	name = Paul Prestidge
	email = paul@storypark.com
[core]
	editor = vim
	excludesfile = /Users/paul/.gitignore_global
  attributesfile = ~/.gitattributes
	autocrlf = false
[push]
	default = simple
[alias]
  compare = log --left-right --graph --cherry-pick --oneline
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[help]
	autocorrect = 20
[credential]
	helper = osxkeychain
[interactive]
	diffFilter = diff-highlight
[rebase]
	autoStash = true
[commit]
	template = /Users/paul/.stCommitMsg
