[color]
  ui = true
[user]
  name = Paul Prestidge
  email = paul@paperkite.co.nz
[core]
	editor = vim
	excludesfile = /Users/paul/.gitignore
	autocrlf = false
[push]
	default = simple
[alias]
  compare = log --left-right --graph --cherry-pick --oneline
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[help]
	autocorrect = 20
[credential]
	helper = osxkeychain
[interactive]
	diffFilter = diff-highlight
[rebase]
	autoStash = true
[commit]
	gpgsign = false
